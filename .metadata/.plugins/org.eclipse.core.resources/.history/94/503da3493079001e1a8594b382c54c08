package whiletest;

public class whileTest1 {

	public static void main(String[] args) {
		// 
//		while(조건식) {
//			실행문;
//			증감식;
		
		int sum = 0;
		sum += 1;
		sum += 1;
		sum += 1;
		sum += 1;
		sum += 1;
		sum += 1;
		sum += 1;
		sum += 1;
		sum += 1;
		sum += 10;
		System.out.println(sum);
		int i = 0;
		sum = 0;
		while(i<=10) {
			sum+=i;
			i++;
		}
		System.out.println(sum);
	}

}
