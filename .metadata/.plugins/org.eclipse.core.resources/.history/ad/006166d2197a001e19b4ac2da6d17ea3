package reference;

class subject {
	int subjectNum;
	String subjectTitle;
	String teacherName;
	String score;
}
public class Student {
	int studentId;
	String studentName;
	subject korea;
	subject math;
	subject en;
	subject java;
	
	public static void main(String [] args) {
		Student kim = new Student();
		kim.korea.subjectTitle = "국어";
		System.out.println(kim.korea.subjectTitle);
	}
}
